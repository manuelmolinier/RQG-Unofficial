{
  "name": "runequest",
  "title": "Runequest Glorantha",
  "description": "A fan made ruleset for Runequest Glorantha. Based on Atropos works on worldbuilding systems.",
  "version": 0.1,
  "minimumCoreVersion": "0.5.2",
  "compatibleCoreVersion": "0.5.5",
  "templateVersion": 2,
  "author": "Astaroth",
  "esmodules": ["module/runequest.js"],
  "styles": ["styles/simple.css"],
  "packs": [
    {
      "name": "skills",
      "label": "Skills",
      "system": "runequest",
      "module": "runequest",
      "path": "./packs/skills.db",
      "entity": "Item",
      "tag" : "skill"
    },
    {
      "name": "rolltables",
      "label": "RQG Rolltables",
      "path": "./packs/rolltables.db",
      "entity": "RollTable",
      "module": "runequest"
    }    
  ],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json"
    },
    {
      "lang": "fr",
      "name": "French",
      "path": "lang/fr.json"
    }

  ],
  "gridDistance": 5,
  "gridUnits": "ft",
  "primaryTokenAttribute": "attributes.hitpoints",
  "secondaryTokenAttribute": "attributes.magicpoints",
  "url": "https://gitlab.com/manuelmolinier/runequest-foundryvtt",
  "manifest": "https://gitlab.com/manuelmolinier/runequest-foundryvtt/-/raw/master/system.json",
  "download": "https://gitlab.com/manuelmolinier/runequest-foundryvtt/-/archive/v0.1/runequest-foundryvtt-v0.1.zip",
  "license": "LICENSE.txt"
}
